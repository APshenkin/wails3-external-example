{
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "lib": [
      "es2020",
      "es2022",
      "dom"
    ],
    "types": ["node"],
    "pretty": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "baseUrl": "."
  }
}
